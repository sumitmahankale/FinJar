{
  "name": "finjar-frontend",
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_API_BASE_URL": "https://finjar-backend-xyz.onrender.com"
  },
  "build": {
    "env": {
      "VITE_API_BASE_URL": "https://finjar-backend-xyz.onrender.com"
    }
  }
}
